{
    "name": "Msdyn365-Commerce-Samples",
    "version": "9.49",
    "private": true,
    "workspaces": {
        "packages": [
            "Training Samples/*",
            "Extensibility Samples/*"
        ]
    },
    "scripts": {
        "analyze": "SET ANALYZE_BUNDLE=true && yarn msdyn365b start --use-eslint",
        "analyze:prod": "SET ANALYZE_BUNDLE=true && yarn msdyn365b build --use-eslint",
        "build": "SET NODE_OPTIONS=--max_old_space_size=4096 && yarn msdyn365b build --use-eslint",
        "build:prod": "yarn clean && SET NODE_OPTIONS=--max_old_space_size=4096 && yarn msdyn365b build --use-eslint",
        "clean": "yarn rimraf build lib dist .tmp gendef.lock",
        "format": "yarn prettier **/**.{ts,tsx}",
        "format:fix": "yarn prettier **/**.{ts,tsx} --write",
        "lint": "yarn eslint src/**/*.{ts,tsx}",
        "lint:fix": "yarn eslint src/**/*.{ts,tsx} --fix",
        "precommit": "lint-staged",
        "start": "SET NODE_OPTIONS=--max_old_space_size=4096 && yarn msdyn365b start local --use-eslint",
        "start:prod": "SET NODE_ENV=production && node build/server.js"
    },
    "devDependencies": {
        "@msdyn365-commerce/eslint-config": "^1.0.0",
        "@types/fs-extra": "^5.0.4",
        "@types/jest": "^23.3.10",
        "@types/node": "18.11.9",
        "@types/node-fetch": "^2.1.4",
        "@types/react": "16.9.0",
        "@types/react-dom": "16.0.11",
        "@types/react-test-renderer": "^16.0.2",
        "@types/reactstrap": "^6.4.3",
        "@typescript-eslint/eslint-plugin": "^4.10.0",
        "@typescript-eslint/parser": "^4.10.0",
        "deep-equal": "1.0.1",
        "eslint": "^7.30.0",
        "eslint-config-prettier": "^7.0.0",
        "eslint-plugin-header": "^3.1.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsdoc": "^30.7.8",
        "eslint-plugin-no-null": "^1.0.2",
        "eslint-plugin-prettier": "^3.3.0",
        "eslint-plugin-react": "^7.21.5",
        "fs-extra": "^7.0.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^23.6.0",
        "jest-junit": "^5.2.0",
        "lint-staged": "^7.3.0",
        "node-fetch": "2.6.1",
        "prettier": "^1.15.3",
        "react-test-renderer": "^16.4.2",
        "semver": "^5.6.0",
        "ts-jest": "^23.10.5",
        "typeson": "5.10.1",
        "husky": "^1.3.1",
        "lerna": "3.13.4",
        "npm-run-all": "^4.1.5",
        "shelljs": "^0.8.2",
        "yargs": "^12.0.5",
        "rimraf": "^2.6.3",
        "@types/qrcode.react": "1.0.1"
    },
    "dependencies": {
        "@msdyn365-commerce-modules/starter-pack": "9.49",
        "@msdyn365-commerce-modules/fabrikam-design-kit": "9.49",
        "@msdyn365-commerce/bootloader": "^1.0.0",
        "@msdyn365-commerce/retail-proxy": "9.49.5",
        "@msdyn365-commerce-theme/adventureworks-theme-kit": "9.49",
        "@msdyn365-commerce-modules/msdyn365-exp-test-connector": "^1.0.0",
        "reactstrap": "^6.5.0",
        "stack-trace": "^0.0.10",
        "tslib": "^1.9.3",
        "path-browserify": "1.0.1",
        "stream-browserify": "3.0.0",
        "browserify-fs": "1.0.0",
        "util": "0.12.3",
        "react-universal-component": "^4.0.0",
        "@babel/runtime": "^7.12.5",
        "run-script-webpack-plugin": "0.0.5",
        "terser": "5.14.2",
        "terser-webpack-plugin": "5.1.1",
        "process": "0.11.10",
        "sass-loader": "^10.2.0",
        "css-loader": "^2.1.1",
        "node-sass": "^6.0.1",
        "mini-css-extract-plugin": "^0.5.0",
        "css-minimizer-webpack-plugin": "3.1.3",
        "ignore-emit-webpack-plugin": "2.0.6",
        "qrcode.react": "1.0.1"
    },
    "lint-staged": {
        "*.(j|t)s(x)?": [
            "yarn format:fix",
            "yarn lint:fix",
            "git add"
        ]
    },
    "jest": {
        "preset": "@msdyn365-commerce/cli-internal"
    },
    "resolutions": {
        "@msdyn365-commerce/bootloader": "^1.0.0",
